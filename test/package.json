{
	"name": "test",
	"version": "0.0.0",
	"private": true,
	"jest": {
		"moduleFileExtensions": [
			"js"
		],
		"testMatch": [
			"./**/*.test.js"
		],
		"coverageDirectory": "../doc/jest",
		"coverageReporters": [
			"lcov"
		],
		"collectCoverageFrom": [
			"../**/*.js",
			"!../**/*.test.js"
		]
	},
	"scripts": {
		"test": "jest --coverage=false --watch",
		"test-ci": "jest --coverage=false --verbose",
		"coverage": "rm -rf ../doc/jest && jest --coverage --silent",
		"build": "node-gyp rebuild",
		"preinstall": "cd .. && npm ci"
	},
	"dependencies": {
		"jest": "26.6.3"
	}
}
